<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/MainUI.uss?fileID=7433441132597879392&amp;guid=90ba3922450cc9f7b982d98aec5a4d53&amp;type=3#MainUI" />
    <engine:VisualElement name="ContainerRoot" style="flex-grow: 1; flex-direction: row; -unity-font-definition: url(&quot;project://database/Assets/Fonts/Raleway-Medium.ttf?fileID=12800000&amp;guid=6471bbe56b8895b4bb6c5a665ba7cb9d&amp;type=3#Raleway-Medium&quot;);">
        <engine:VisualElement name="ContainerLeft" class="left-panel" style="background-color: rgba(0, 0, 0, 0.86); padding-left: 10px; padding-right: 10px; height: auto; font-size: 42%;">
            <engine:Label text="SPH Simulation" name="LabelColorPicker" emoji-fallback-support="false" class="header" style="display: flex; justify-content: flex-start; align-self: center; font-size: 36px;" />
            <engine:VisualElement name="AreaActions" class="section-container" style="align-items: auto; height: auto;">
                <SPHSimulator.UI.StartPauseButton start-icon="project://database/Assets/Icons/play.png?fileID=2800000&amp;guid=3ef7457261b7727baa61e1842bb250f8&amp;type=3#play" pause-icon="project://database/Assets/Icons/pause.png?fileID=2800000&amp;guid=7c9a57cb26fd455e8accfd755abfadee&amp;type=3#pause" is-running="true" name="ButtonStartPause" class="action-button icon-button" />
                <engine:Button name="ButtonReset" icon-image="project://database/Assets/Icons/reset.png?fileID=2800000&amp;guid=6c53a295dc08bac52b01d47de363524e&amp;type=3#reset" parse-escape-sequences="true" class="action-button icon-button" />
                <engine:Button icon-image="project://database/Assets/Icons/floppy.png?fileID=2800000&amp;guid=932860725baee7e99ad88e86d05850e1&amp;type=3#floppy" name="ButtonSave" class="action-button icon-button" />
                <engine:Button icon-image="project://database/Assets/Icons/folder.png?fileID=2800000&amp;guid=0f749291ce0e094ada16204de9914369&amp;type=3#folder" name="ButtonLoad" class="action-button icon-button" />
            </engine:VisualElement>
            <engine:Label name="LabelActionNotifications" />
            <engine:Label text="Obstacles" name="LabelObstacles" emoji-fallback-support="false" tooltip="Generate obstacles to redirect or block the flow of the liquid." class="header" style="display: flex;" />
            <engine:VisualElement name="ContainerObstacles" class="section-container" style="display: flex; height: 180px; flex-direction: column;">
                <engine:VisualElement class="section-container" style="flex-grow: 1; height: 90%; background-color: rgba(0, 0, 0, 0); justify-content: space-between;">
                    <engine:VisualElement name="IconButtonWithLabel" class="icon-button-with-label" style="align-items: center; align-self: flex-start;">
                        <engine:Button name="ButtonObstacleCube" tooltip="Create a cube obstacle to redirect or block the flow of the liquid." class="icon-button" style="justify-content: flex-start; align-self: center;" />
                        <engine:Label text="Cube" />
                    </engine:VisualElement>
                    <engine:VisualElement name="IconButtonWithLabel" class="icon-button-with-label" style="align-items: center; align-self: flex-start;">
                        <engine:Button name="ButtonObstacleSphere" tooltip="Create a sphere obstacle to redirect or block the flow of the liquid." class="icon-button" style="justify-content: flex-start; align-self: center;" />
                        <engine:Label text="Sphere" />
                    </engine:VisualElement>
                    <engine:VisualElement name="IconButtonWithLabel" class="icon-button-with-label" style="align-items: center; align-self: flex-start;">
                        <engine:Button name="ButtonObstacleCapsule" tooltip="Create a capsule obstacle to redirect or block the flow of the liquid." class="icon-button" style="justify-content: flex-start; align-self: center;" />
                        <engine:Label text="Capsule" />
                    </engine:VisualElement>
                </engine:VisualElement>
                <engine:Button text="Delete all obstacles" name="ButtonDeleteAllObstacles" enabled="true" class="delete-button" />
            </engine:VisualElement>
            <engine:Label text="Fluid color" name="LabelColorPicker" emoji-fallback-support="false" class="header" style="display: flex;" />
            <engine:VisualElement name="ContainerColorPicker" class="section-container" style="display: flex; justify-content: space-between; height: auto; flex-direction: column;">
